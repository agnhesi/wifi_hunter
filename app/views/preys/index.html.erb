<h1>Listing preys</h1>

<table>
  <tr>
    <th>Person name</th>
    <th>Person email</th>
    <th>Country</th>
    <th>State</th>
    <th>City</th>
    <th>Place</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @preys.each do |prey| %>
  <tr>
    <td><%= prey.person_name %></td>
    <td><%= prey.person_email %></td>
    <td><%= prey.country %></td>
    <td><%= prey.state %></td>
    <td><%= prey.city %></td>
    <td><%= prey.place %></td>
    <td><%= link_to 'Show', prey %></td>
    <td><%= link_to 'Edit', edit_prey_path(prey) %></td>
    <td><%= link_to 'Destroy', prey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Prey', new_prey_path %>
